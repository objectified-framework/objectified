{
  "name": "@objectified-framework/objectified-api",
  "version": "0.1.13",
  "description": "Objectified OpenAPI Toolset",
  "scripts": {
    "clean": "rm -rf dist cfx src/generated",
    "lint": "npx @redocly/cli lint ../openapi/openapi.yaml",
    "build": "yarn clean && tsc && mkdir dist/.bin dist/assets && cp LICENSE dist && cp src/generator/nestjs/templates/* dist/assets && cp bin/gen.sh dist/.bin ",
    "prettier": "npx prettier -w src",
    "release": "npm run build && npm publish"
  },
  "bin": "./bin/gen.sh",
  "author": "ksuenobu@fastmail.com",
  "license": "Apache License 2.0",
  "dependencies": {
    "@faker-js/faker": "^9.2.0",
    "@nestjs/common": "^10.4.12",
    "@nestjs/core": "^10.4.12",
    "@nestjs/swagger": "^8.0.7",
    "@redocly/cli": "^1.26.1",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "axios": "^1.7.9",
    "commander": "^12.1.0",
    "pg-promise": "^11.10.2",
    "rxjs": "^7.8.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.7.2",
    "yaml": "^2.6.1"
  },
  "files": [
    "bin",
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@types/babel__preset-env": "^7",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.9.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "json-schema-faker": "^0.5.6",
    "prettier": "^3.4.1",
    "reflect-metadata": "^0.2.2"
  }
}
